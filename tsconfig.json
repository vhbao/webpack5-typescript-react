{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "jsx": "preserve",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "baseUrl": "./src",
    "noImplicitAny": false,
    "resolveJsonModule": true,    
    "paths": {
      "components/*": ["components/*"],
      "assets/*": ["assets/*"],
      "jsonconfig/*": ["jsonconfig/*"],
      "jsondata/*": ["jsondata/*"],
      "layouts/*": ["layouts/*"],
      "store/*": ["store/*"],
      "constant/*": ["constant/*"],
      "routes/*": ["routes/*"],
      "api/*": ["api/*"],
      "common/*": ["common/*"],
    },
  },
  "include": [
    "src"
  ],
}
